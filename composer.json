{
    "name": "makise-co/fx",
    "description": "Framework that makes it easy to build applications out of reusable, composable modules.",
    "keywords": [
        "framework",
        "makise",
        "fx"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry K.",
            "email": "coder1994@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "suggest": {

    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "monolog/monolog": "^2.2",
        "php-di/php-di": "^6.3",
        "psr/container": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "amphp/amp": "^2.5",
        "phpstan/phpstan": "^0.12.18",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpunit/phpunit": "^9.0",
        "react/event-loop": "^1.1",
        "react/promise": "^2.8",
        "swoole/ide-helper": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "MakiseCo\\Fx\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakiseCo\\Fx\\Tests\\": "tests/"
        }
    }
}
